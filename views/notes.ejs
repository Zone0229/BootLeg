
    <div class="jumbotron">
        <center><b><h1 class="tnr">Notes</h1></b></center>
    </div>
    <table class="table-bordered table-striped">
      <% for(i = 0; i<notes.length; i++){  %>
          <tr>
            <td>
              <a href="/showNote/<%= notes[i]._id %>">
                <%= notes[i].header %></a>
              </td>
                <td>
              <%= notes[i].text %>
            </td>
               <td>
              <%= notes[i].textbook %>
            </td>
              <td>
                <%= notes[i].postdate %>
              </td>
              <td>
                <%= notes[i].page1 %> - <%= notes[i].page2 %>
              </td>
             </tr>
      <% } %>
    </table>
